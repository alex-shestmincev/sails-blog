<div class="container text-center">
  <h2 class="text-center">MY BLOG APP</h2>
  <div class="row">
    <div class="col-md-1"></div>
    <div class="col-md-10">
      <% _.each(articles, function (article) { %>
      <div class="panel panel-default">
        <div class="panel-body">
          <h3 class="text-center"><%= article.title %></h3><hr>
          <article>
            <%= article.description %>
          </article>
        </div>
        <div class="panel-footer">
          <a href="/article/watch/<%= article.id %>" class="btn btn-info">LEARN MORE</a>
        </div>
      </div>
      <% }) %>
    </div>
    <div class="col-md-1">
    </div>
    <ul class="pagination">
      <li><a href="/article">1</a></li>
      <li><a href="/article/2">2</a></li>
      <li><a href="/article/3">3</a></li>
    </ul>
  </div>
